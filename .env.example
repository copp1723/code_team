# Multi-Agent Orchestrator Environment Configuration

# OpenRouter API Key (Required for AI features)
# Get your key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your-actual-key-here

# Project Configuration
PROJECT_PATH=../test-project
MASTER_BRANCH=main

# Git Configuration
GIT_REMOTE_URL=https://github.com/YOUR_USERNAME/YOUR_REPO.git

# Agent Configuration
MAX_CONCURRENT_AGENTS=3
SYNC_INTERVAL=30m

# AI Model Preferences (optional - defaults are set in ai-agent-engine.js)
AI_MODEL_FRONTEND=anthropic/claude-3-sonnet-20240229
AI_MODEL_BACKEND=openai/gpt-4-turbo-preview
AI_MODEL_DATABASE=anthropic/claude-3-opus-20240229

# Resource Limits
MAX_TOKENS_PER_HOUR=100000
MAX_COST_PER_DAY=50.00

# Web Interface
WEB_PORT=8080
ENABLE_WEBSOCKET=true

# Logging
LOG_LEVEL=info
LOG_TO_FILE=true
