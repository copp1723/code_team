# Multi-Agent Orchestrator Environment Configuration

# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Supermemory API Configuration (Optional)
SUPERMEMORY_API_KEY=your_supermemory_api_key_here
SUPERMEMORY_BASE_URL=https://api.supermemory.ai/v1

# Server Configuration
PORT=8080
NODE_ENV=production

# Dashboard Configuration
AUTO_REFRESH_INTERVAL=5000
MAX_LOG_ENTRIES=100
MAX_CONVERSATION_HISTORY=20

# AI Model Preferences
DEFAULT_MODEL=anthropic/claude-4-sonnet
FAST_MODEL=openai/gpt-4-turbo-preview

# Security Configuration
ALLOWED_ORIGINS=http://localhost:8080,http://127.0.0.1:8080
SESSION_SECRET=your_session_secret_here

# File Paths
TICKETS_FILE=../tickets.txt
AGENT_CONFIG_FILE=../agent-orchestrator.config.json
STATE_FILE=../.master-dispatcher-state.json

# Resource Limits
MAX_BUFFER_SIZE=1048576
COMMAND_TIMEOUT=30000
MAX_CONCURRENT_PROCESSES=10
